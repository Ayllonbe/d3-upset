  "bugs": {
    "url": "https://github.com/dropofwill/d3-upset/issues"
  },
  "homepage": "https://github.com/dropofwill/d3-upset",
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.1",
    "glob": "^6.0.3",
    "json-loader": "^0.5.3",
    "lodash": "^4.11.1",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "ramda": "^0.21.0"
