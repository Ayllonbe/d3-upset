{
  "name": "d3-upset",
  "version": "0.0.1",
  "description": "A minimal D3 implementation of the UpSet visualization",
  "main": "dist/d3-upset.js",
  "scripts": {
    "build": "webpack -p",
    "build-dev": "webpack -d",
    "test": "mocha --compilers js:babel-register test/setup/node.js test/unit/**/*.js",
    "test:watch": "npm run test -- -w",
    "build-browser-tests": "webpack -d --config webpack.tests.config.js",
    "build-browser-tests:watch": "npm run build-browser-tests -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dropofwill/d3-upset.git"
  },
